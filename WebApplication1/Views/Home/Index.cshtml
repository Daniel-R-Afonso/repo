                            credentials: "Ai2YaUnWLHGqJ3qMvxN4VRFrWbd4l8WBbxeB0nn8msfSss9xUSne2__s_gmGcSBg",
                            enableClickableLogo: false,
                            showDashboard: false,
                            zoom: 7,
                            center: new Microsoft.Maps.Location(39.68594535185488, -8.491974121657469),
                            labelOverlay: Microsoft.Maps.LabelOverlay.hidden,
                            mapTypeId: Microsoft.Maps.MapTypeId.road
                    });
                 }
                 LoadMap();
            });
            
        </script>
        <div id="myMap" style="position:relative; width:900px; height:650px;">

        </div>
