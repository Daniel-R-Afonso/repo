<body onload="OnPageLoad()">
    <form action="">
        

        <script language="javascript" type="text/javascript">
            $(document).ready(function () {
                var map = null;
                function LoadMap() {
                    map = new Microsoft.Maps.Map(
                        document.getElementById('myMap'),
